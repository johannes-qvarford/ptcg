                    console.log('gpt2 width ' + window.innerWidth);
                    if (window.innerWidth >= 1370) {
                        skyRight.id = 'div-gpt-ad-1498167603841-2';
                        skyRightContainer.appendChild(skyRight);
                        //var zergRight = document.createElement('div');
                        //zergRight.id = 'zergnet-widget-43478';
                        //skyRightContainer.appendChild(zergRight);
                        skyRightContainer.id = 'rightsky';
                    }
                } else {
                    skyRight.id = 'div-gpt-ad-1512651376746-0';
                    googletag.cmd.push(function () { googletag.display('div-gpt-ad-1512651376746-0'); });
                }
                var globalwrapper = document.getElementById('globalWrapper');
                document.body.insertBefore(skyLeft, globalwrapper);
                document.body.insertBefore(skyRightContainer, globalwrapper);

                //Zergnet
                //if (window.innerWidth >= 1675) {
                //    console.log('getting zerg');
                //    (function () {
                //        var zergnet = document.createElement('script');
                //        zergnet.type = 'text/javascript'; zergnet.async = true;
                //        zergnet.src = 'https://www.zergnet.com/zerg.js?id=43478';
                //        var znscr = document.getElementsByTagName('script')[0];
                //        znscr.parentNode.insertBefore(zergnet, znscr);
                //    })();
                //}
            }
        }
        //document.body.insertBefore(gtagbopen, document.body.firstChild);
    }
</script>










